{
    "name": "digital-creative/nova-filepond",
    "description": "A Nova 5 field for uploading File, Image and Video using filepond.",
    "keywords": [
        "file",
        "upload",
        "video",
        "laravel",
        "nova",
        "nova5",
        "field"
    ],
    "authors": [
        {
            "name": "Matt Phillips"
        }
    ],
    "license": "MIT",
    "require": {
        "php": ">=8.2",
        "laravel/nova": "^4.0|^5.0"
    },
    "autoload": {
        "psr-4": {
            "Trscca\\Filepond\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Trscca\\Filepond\\FilepondServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
